{
    "manifest_version": 3,
    "name": "chrome-ext-react-template",
    "description": "desc",
    "version": "1.0.0",
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "action": {
        "default_title": "chrome-ext-react-template",
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://example.com/*",
        "https://*/*"
    ]
}